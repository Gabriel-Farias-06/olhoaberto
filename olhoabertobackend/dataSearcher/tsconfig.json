{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "rootDir": "src",
    "outDir": "dist"
  },
  "ts-node": {
    "files": true
  },
  "include": ["src/**/*", "dataSearcher/express.d.ts"],
  "exclude": ["node_modules", "dist"]
}
